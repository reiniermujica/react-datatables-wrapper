{
  "name": "react-datatables-wrapper",
  "version": "0.0.2",
  "description": "React Datatables.net wrapper",
  "main": "lib/DataTable.js",
  "scripts": {
    "start": "cd examples && node server.js",
    "watch": "cd examples && webpack --watch",
    "build": "cd examples && webpack -p",
    "compile": "babel -d lib/ src/",
    "prepare": "npm run compile",
    "dev": "npm run compile && npm start",
    "clean": "del-cli package-lock.json && del-cli -f node_modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/reiniermujica/react-datatables-wrapper.git"
  },
  "keywords": [
    "react-component",
    "datatables",
    "tables"
  ],
  "dependencies": {
    "datatables.net-dt": "1.10.19",
    "lodash": "4.17.10",
    "prop-types": "15.6.1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "del-cli": "^1.1.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "3.3.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "author": "Reinier Mujica <reinier.mujica@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reiniermujica/react-datatables-wrapper/issues"
  },
  "homepage": "https://github.com/reiniermujica/react-datatables-wrapper"
}
